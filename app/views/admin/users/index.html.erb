<h2>Current Users</h2>
<% @charities.each do |charity| %>
  <div class="<%= charity.name %>">
  <h4><%= charity.name %></h4>
  <ul>
  <% charity.users.each do |user| %>
    <li><%= user.username %></li>
    <!-- <li> <%#= user. %> </li> -->
    <% end %>
  </ul>
  <% end %>
</div>
<h2>All Users</h2>
<ul>
  <% @users.each do |user| %>
  <li><%= link_to user.username, admin_user_path(user) %></li>
  <% end %>
</ul>
