<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <div class="panel panel-default">

      <div class="panel-heading">
        <h3 class="panel-title text-center">Submit Your Charity</h3>
      </div>

      <div class="panel-body">

          <%= form_for @charity, multipart: true do |f| %>

          <%= render partial: "/shared/charity_form", locals: {f: f}  %>

          <%= f.submit "Submit Charity", class: "btn btn-primary" %>
        <% end %>
      </div>
    </div>
  </div>
</div>
